<h1>All playgrounds</h1>

  <% @playgrounds.each do |playground| %>
    <%= playground.name %>
    <%= playground.address %>
    <%= playground.rims_number %>,
    <%= playground.city %>
    <%= link_to 'go', playground_path(playground) %>
    <hr>
  <% end %>


  <%= link_to 'Add a playground', new_playground_path %>
